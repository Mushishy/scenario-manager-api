{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "type": { "type": "string", "enum": ["CTFD", "SHARED", "INDIVIDUAL"] },
        "mainUser": { "type": "string" },
        "usersAndTeams": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "user": { "type": "string" },
                    "team": { "type": "string" }
                },
                "required": ["user"],
                "additionalProperties": false
            }
        },
        "topologyId": { "type": "string" },
        "note": { "type": "string", "maxLength": 15 }
    },
    "required": ["type", "topologyId", "note"],
    "additionalProperties": false
}