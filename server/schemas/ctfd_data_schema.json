{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "ctfd_data": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "user": { "type": "string" },
                    "password": { "type": "string" },
                    "team": { "type": "string" },
                    "flags": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "variable": { "type": "string" },
                                "contents": { "type": "string" }
                            },
                            "required": ["variable", "contents"],
                            "additionalProperties": false
                        }
                    }
                },
                "required": ["user", "password", "flags"],
                "additionalProperties": false
            }
        }
    },
    "required": ["ctfd_data"],
    "additionalProperties": false
}